{
  "name": "jorijn/symfony-bunq-bundle",
  "description": "A Symfony 3.4 LTS compatible Bundle for interaction with the European bunq bank",
  "type": "library",
  "require": {
    "symfony/symfony": "3.4.*",
    "bunq/sdk_php": "0.13.*",
    "php": "^7.0.13",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.1 <8.0.0",
    "friendsofphp/php-cs-fixer": "^2.11"
  },
  "autoload": {
    "psr-4": {
      "Jorijn\\SymfonyBunqBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Jorijn\\SymfonyBunqBundle\\": "Tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "license": "MIT",
  "scripts": {
    "test": "bin/phpunit --coverage-text;"
  },
  "authors": [
    {
      "name": "Jorijn Schrijvershof",
      "email": "jorijn@jorijn.com"
    }
  ]
}
